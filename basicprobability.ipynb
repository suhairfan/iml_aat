      "source": [
        "from fractions import Fraction\n",
        "\n",
        "X = ['w']*5 + ['b']*4\n",
        "Y = ['w']*7 + ['b']*6\n",
        "count=0\n",
        "visited=0\n",
        "\n",
        "for i in X:\n",
        "    tem=Y+[i]\n",
        "    for j in tem:\n",
        "        if j==\"b\":\n",
        "            count+=1\n",
        "        visited+=1\n",
        "\n",
        "print(Fraction(count, visited))"
      ]
    }
  ]
}